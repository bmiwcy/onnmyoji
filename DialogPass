function dot3()
keepScreen(true)
x, y = findMultiColorInRegionFuzzy(0xfde8e8,"-6|-7|0x2e1714,12|-11|0x1c130f,10|13|0x18110d,-10|16|0x12100c,13|23|0x12100c", 95, 10, 10, 1135, 490)
if x>-1 then
toast("dot3!!!")
touchDown(0, x, y)
mSleep(50)
touchUp(0, x, y)
x = -1
y = -1
end
keepScreen(false)
end


function question()
keepScreen(true)
